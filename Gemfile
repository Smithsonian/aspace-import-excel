ASpaceGems.setup if defined? ASpaceGems

source 'http://rubygems.org'


def pry_gem
    vers = '~> 0.10.4'
    if defined?(ASConstant.VERSION)
       vers_arr = ASConstant.VERSION.split(".")
       if vers_ar[0] == 'v2' && ver_ar[1] == "2"  && ver_ar[2].start__with?("2")
	   vers =  '~> 0.11.3' 
       end 
    end
    gem 'pry', vers
end

pry_gem()
gem 'rubyXL', "3.1.0", :require => false

